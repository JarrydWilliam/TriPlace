{
  "name": "TriPlace - Your Digital Third Place",
  "short_name": "TriPlace",
  "description": "Where genuine connection, shared passions, and meaningful community are always just around the corner.",
  "start_url": "/",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#268, 83%, 58%",
  "background_color": "#220, 25%, 8%",
  "categories": ["social", "lifestyle", "productivity"],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "maskable any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "maskable any"
    }
  ],
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Home",
      "description": "Go to your dashboard",
      "url": "/dashboard"
    },
    {
      "name": "Communities",
      "short_name": "Communities", 
      "description": "Browse your communities",
      "url": "/communities"
    }
  ],
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "prefer_related_applications": false,
  "scope": "/",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  }
}